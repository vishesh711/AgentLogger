{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/app/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ],
  "env": {
    "ENVIRONMENT": "production",
    "API_V1_STR": "/api/v1"
  },
  "build": {
    "env": {
      "PYTHON_VERSION": "3.11"
    }
  },
  "builds": [
    {
      "src": "app/main.py",
      "use": "@vercel/python"
    },
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ]
} 