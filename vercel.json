{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/app/main.py"
    },
    {
      "source": "/(.*)",
      "destination": "/frontend/$1"
    }
  ],
  "env": {
    "ENVIRONMENT": "production",
    "API_V1_STR": "/api/v1",
    "DATABASE_URL": "@database_url",
    "GROQ_API_KEY": "@groq_api_key",
    "SECRET_KEY": "@secret_key",
    "CORS_ORIGINS": "[\"https://agentlogger.vercel.app\"]"
  },
  "build": {
    "env": {
      "PYTHON_VERSION": "3.11"
    }
  },
  "functions": {
    "app/main.py": {
      "maxDuration": 30
    }
  }
} 